<library path="lib/libmavros_px4_quadrotor_sim_nodelet" >
  <!-- name: launch文件里面 load 后面接着的插件名
       type: c++文件定义的类名
       如 name="aaa/nodeletclass1",那么，launch文件对应启动如下：
       <node pkg="nodelet" type="nodelet" name="nodeletclass1"
       args="load aaa/nodeletclass1 nodelet_manager" output="screen">
  -->
  <class name="mavros_px4_quadrotor_sim_nodelet" type="MavrosQuadSimulator::SimAgent" base_class_type="nodelet::Nodelet">
    <description>
        Simulation node of Mavros, PX4 and rotor dynamics.
    </description>
  </class>
</library>